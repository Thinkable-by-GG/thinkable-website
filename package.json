{
  "name": "thinkable-website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload generate:types",
    "seed:studies": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts npx tsx src/cms/seed-studies.ts",
    "transform:studies": "npx tsx src/cms/import-studies-api.ts"
  },
  "dependencies": {
    "@mantine/carousel": "^7.17.8",
    "@mantine/core": "^7.5.2",
    "@mantine/form": "^7.5.2",
    "@mantine/hooks": "^7.5.2",
    "@mantine/notifications": "^7.5.2",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-mongodb": "^1.0.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@tabler/icons-react": "^2.47.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.18.2",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "mongodb": "^6.3.0",
    "openai": "^6.8.1",
    "payload": "^2.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.12.3",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
